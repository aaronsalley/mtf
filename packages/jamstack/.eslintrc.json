{
  "extends": [
    "plugin:prettier/recommended",
    "prettier",
    "next/babel",
    "next/core-web-vitals"
  ],
  "settings": {
    "next": {
      "rootDir": "packages/jamstack/"
    }
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off",
    "react/no-unknown-property": ["error", { "ignore": ["key"] }]
  }
}
