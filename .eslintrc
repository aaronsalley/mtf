{
  "extends": [
    "plugin:react/recommended",
    "google",
  ],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 2017,
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
  },
  "plugins": [
    "babel",
  ],
  "rules": {
      "max-len": "warn",
      "no-invalid-this": "warn",
  },
  "settings": {
    "react": {
      "version": "15.0",
    }
  }
}
